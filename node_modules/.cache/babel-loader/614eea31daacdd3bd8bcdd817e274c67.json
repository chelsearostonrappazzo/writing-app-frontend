AAA,EAAA,GAAA,WAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAHA;AAIA,KAZA;AAaA,IAAA,aAAA,EAAA,uBAAA,OAAA,EAAA;AAAA;;AACA,WAAA,iBAAA,GAAA,OAAA,CAAA,EAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,eAAA,KAAA,iBAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,cAAA,GAAA,MAAA,CAAA,OAAA;AACA,OAJA;AAKA,KApBA;AAqBA,IAAA,YAAA,EAAA,wBAAA;AACA,MAAA,QAAA,CAAA,aAAA,CAAA,oBAAA,EAAA,SAAA;AACA,KAvBA;AAwBA,IAAA,UAAA,EAAA,sBAAA;AAAA;;AACA,UAAA,MAAA,GAAA;AACA,QAAA,KAAA,EAAA,KAAA,KADA;AAEA,QAAA,QAAA,EAAA,KAAA,MAAA,CAAA,MAAA,CAAA;AAFA,OAAA;AAIA,MAAA,KAAA,CAAA,IAAA,CAAA,cAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,GAAA,WAAA,EAAA,MAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,IAAA;AACA,OAHA;AAIA;AAjCA;AAjBA,CAAA","sourcesContent":["<template>\n  <div class=\"container editor\">\n    <div class=\"row\">\n      <div class=\"col-md-4\">\n        <div class=\"btn-group\" role=\"group\">\n          <button type=\"button\" class=\"btn btn-primary btn-group-sm outline\" v-on:click=\"chapterModal()\">\n            +Chapter\n          </button>\n          <button type=\"button\" class=\"btn btn-primary btn-group-sm outline\">+Character</button>\n          <button type=\"button\" class=\"btn btn-primary btn-group-sm outline\">+ Party Member</button>\n        </div>\n        <dialog id=\"add-chapter-modal\">\n          <form method=\"dialog\">\n            <label>Title</label>\n            <input type=\"text\" v-model=\"title\" />\n            <button v-on:click=\"newChapter()\" class=\"btn btn-primary outline\">Add</button>\n            <button class=\"btn btn-primary outline\">Close</button>\n          </form>\n        </dialog>\n        <div class=\"list-group\">\n          <a class=\"list-group-item\" v-for=\"chapter in chapters\" :key=\"chapter.id\" v-on:click=\"selectChapter(chapter)\">\n            {{ chapter.title }}\n          </a>\n        </div>\n      </div>\n      <div class=\"col-md-8\">\n        {{ this.displayChapter.body }}\n        <\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { Editor, EditorContent } from \"@tiptap/vue-2\";\nimport StarterKit from \"@tiptap/starter-kit\";\n\nexport default {\n  components: {\n    EditorContent,\n  },\n  data: function () {\n    return {\n      selectedChapterId: \"\",\n      chapters: [],\n      displayChapter: \"\",\n      editor: null,\n      title: \"\",\n    };\n  },\n  mounted: function () {\n    this.indexChapter();\n    this.setupTipTap();\n  },\n  methods: {\n    setUpTipTap: function () {\n      this.editor = new Editor({\n        content: \"<p>{{this.displayChapter}}</p>\",\n        extensions: [StarterKit],\n      });\n    },\n    indexChapter: function () {\n      axios.get(\"/stories/\" + this.$route.params.id + \"/chapters\").then((response) => {\n        console.log(response.data);\n        this.chapters = response.data;\n      });\n    },\n    selectChapter: function (chapter) {\n      this.selectedChapterId = chapter.id;\n      axios.get(\"/chapters/\" + this.selectedChapterId).then((response) => {\n        console.log(response.data);\n        this.chapter = response.data;\n        this.displayChapter = this.chapter;\n      });\n    },\n    chapterModal: function () {\n      document.querySelector(\"#add-chapter-modal\").showModal();\n    },\n    newChapter: function () {\n      let params = {\n        title: this.title,\n        story_id: this.$route.params.id,\n      };\n      axios.post(\"/stories/\" + this.$route.params.id + \"/chapters\", params).then((response) => {\n        console.log(response.data);\n        this.chapters.push(response.data);\n      });\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}