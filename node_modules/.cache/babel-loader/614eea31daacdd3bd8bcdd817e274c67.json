AA;AAQA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,KAXA;AAYA,IAAA,YAAA,EAAA,wBAAA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,cAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,GAAA,WAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAHA;AAIA,KAjBA;AAkBA,IAAA,aAAA,EAAA,uBAAA,OAAA,EAAA;AAAA;;AACA,WAAA,iBAAA,GAAA,OAAA,CAAA,EAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,eAAA,KAAA,iBAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,cAAA,GAAA,MAAA,CAAA,OAAA;AACA,OAJA;AAKA,KAzBA;AA0BA,IAAA,YAAA,EAAA,wBAAA;AACA,MAAA,QAAA,CAAA,aAAA,CAAA,oBAAA,EAAA,SAAA;AACA,KA5BA;AA6BA,IAAA,UAAA,EAAA,sBAAA;AAAA;;AACA,UAAA,MAAA,GAAA;AACA,QAAA,KAAA,EAAA,KAAA,KADA;AAEA,QAAA,QAAA,EAAA,KAAA,MAAA,CAAA,MAAA,CAAA;AAFA,OAAA;AAIA,MAAA,KAAA,CAAA,IAAA,CAAA,cAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,GAAA,WAAA,EAAA,MAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,IAAA;AACA,OAHA;AAIA;AAtCA;AAdA,CAAA","sourcesContent":["<template>\n  <div class=\"container editor\">\n    <div class=\"row\">\n      <div class=\"col-md-4\">\n        <div class=\"btn-group\" role=\"group\">\n          <button type=\"button\" class=\"btn btn-primary btn-group-sm outline\" v-on:click=\"chapterModal()\">\n            +Chapter\n          </button>\n          <button type=\"button\" class=\"btn btn-primary btn-group-sm outline\">+Character</button>\n          <button type=\"button\" class=\"btn btn-primary btn-group-sm outline\">+ Party Member</button>\n        </div>\n        <dialog id=\"add-chapter-modal\">\n          <form method=\"dialog\">\n            <label>Title</label>\n            <input type=\"text\" v-model=\"title\" />\n            <button v-on:click=\"newChapter()\" class=\"btn btn-primary outline\">Add</button>\n            <button class=\"btn btn-primary outline\">Close</button>\n          </form>\n        </dialog>\n        <div class=\"list-group\">\n          <a\n            class=\"list-group-item outline\"\n            v-for=\"chapter in chapters\"\n            :key=\"chapter.id\"\n            v-on:click=\"selectChapter(chapter)\"\n          >\n            {{ chapter.title }}\n          </a>\n        </div>\n      </div>\n      <div class=\"col-md-8\">\n        {{ this.displayChapter.body }}\n        <div id=\"scrolling-container\">\n          <div id=\"quill-container\">\n            <div id=\"editor-container\">\n              <p></p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n/* global Quill */\nimport axios from \"axios\";\n\nexport default {\n  data: function () {\n    return {\n      selectedChapterId: \"\",\n      chapters: [],\n      displayChapter: \"\",\n      quill: null,\n      title: \"\",\n    };\n  },\n  mounted: function () {\n    this.setUpEditor();\n    this.indexChapter();\n  },\n  methods: {\n    setUpEditor: function () {\n      var quill = new Quill(\"#quill-container\", {\n        modules: {\n          toolbar: [[{ header: [1, 2, false] }], [\"bold\", \"italic\", \"underline\"], [\"image\", \"code-block\"]],\n        },\n        scrollingContainer: \"#scrolling-container\",\n        theme: \"snow\",\n      });\n\n      console.log(quill);\n    },\n    indexChapter: function () {\n      axios.get(\"/stories/\" + this.$route.params.id + \"/chapters\").then((response) => {\n        console.log(response.data);\n        this.chapters = response.data;\n      });\n    },\n    selectChapter: function (chapter) {\n      this.selectedChapterId = chapter.id;\n      axios.get(\"/chapters/\" + this.selectedChapterId).then((response) => {\n        console.log(response.data);\n        this.chapter = response.data;\n        this.displayChapter = this.chapter;\n      });\n    },\n    chapterModal: function () {\n      document.querySelector(\"#add-chapter-modal\").showModal();\n    },\n    newChapter: function () {\n      let params = {\n        title: this.title,\n        story_id: this.$route.params.id,\n      };\n      axios.post(\"/stories/\" + this.$route.params.id + \"/chapters\", params).then((response) => {\n        console.log(response.data);\n        this.chapters.push(response.data);\n      });\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}